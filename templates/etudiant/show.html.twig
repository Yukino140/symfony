{% extends 'base.html.twig' %}

{% block title %}Etudiant{% endblock %}

{% block body %}
    <h1>Etudiant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etudiant.id }}</td>
            </tr>
            <tr>
                <th>Nce</th>
                <td>{{ etudiant.nce }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ etudiant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ etudiant.prenom }}</td>
            </tr>
        </tbody>
    </table>

    <button class="btn btn-info" style="margin-left:340px;transform:translate(100px,19px);"><a style="color:black;text-decoration:none;" href="{{ path('app_etudiant_index') }}">back to list</a></button>

    <button class="btn btn-success" style="transform:translate(250px,18px)"><a style="color:black;text-decoration:none;" href="{{ path('app_etudiant_edit', {'id': etudiant.id}) }}">edit</a></button>

    {{ include('etudiant/_delete_form.html.twig') }}
{% endblock %}
